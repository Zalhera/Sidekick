                        Grid.Row="7"
                        Grid.Column="0"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockArmourersWorkshop"
                        Grid.Row="7"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
