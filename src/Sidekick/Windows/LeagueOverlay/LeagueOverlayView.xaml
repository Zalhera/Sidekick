                        Text="{x:Static incursion:IncursionResources.SparringRoom}"/>
                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="1"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockArenaOfValour"
                        Grid.Row="6"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.ArenaOfValour}"/>
                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="2"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockHallOfChampions"
                        Grid.Row="6"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.HallOfChampions}"/>
                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSparringRoomContains"
                        Grid.Row="6"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
