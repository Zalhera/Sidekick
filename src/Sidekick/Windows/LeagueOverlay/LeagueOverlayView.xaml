                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.Armoury}"/>
                    <Rectangle
