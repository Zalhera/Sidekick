<Window
    x:Class="Sidekick.Windows.LeagueOverlay.LeagueOverlayView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Sidekick.Windows.LeagueOverlay"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:r="clr-namespace:Sidekick.Localization.Leagues;assembly=Sidekick.Localization"
    xmlns:betrayal="clr-namespace:Sidekick.Localization.Leagues.Betrayal;assembly=Sidekick.Localization"
    xmlns:incursion="clr-namespace:Sidekick.Localization.Leagues.Incursion;assembly=Sidekick.Localization"
    xmlns:delve="clr-namespace:Sidekick.Localization.Leagues.Delve;assembly=Sidekick.Localization"
    xmlns:blight="clr-namespace:Sidekick.Localization.Leagues.Blight;assembly=Sidekick.Localization"
    xmlns:metamorph="clr-namespace:Sidekick.Localization.Leagues.Metamorph;assembly=Sidekick.Localization"
    Width="1200"
    Height="980"
    AllowsTransparency="True"
    Background="Transparent"
    BorderThickness="1"
    ResizeMode="NoResize"
    ShowInTaskbar="False"
    Topmost="True"
    WindowStyle="None"
    mc:Ignorable="d">

    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <SolidColorBrush x:Key="HighValueColor" Color="#FF336303" />
            <SolidColorBrush x:Key="MediumValueColor" Color="#FFBFA500" />
            <SolidColorBrush x:Key="LowValueColor" Color="#FFDA4500" />
            <SolidColorBrush x:Key="VeryLowValueColor" Color="#FFB00000" />
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <TabControl
            x:Name="tabControlLeagueOverlay"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Background="{DynamicResource SolidColorBrush_Background}"
            BorderBrush="{DynamicResource SolidColorBrush_Primary}"
            Foreground="White">
            <TabItem
                x:Name="tabItemIncursion"
                Background="{DynamicResource SolidColorBrush_Background}"
                BorderBrush="{DynamicResource SolidColorBrush_Primary}"
                Header="Incursion">
                <Grid x:Name="gridIncursion"
                    Margin="0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Background="{DynamicResource SolidColorBrush_Background}">
                    <Grid.RowDefinitions>
                        <RowDefinition Name="rowHeader" Height="*" />
                        <RowDefinition Name="rowGuardhouse" Height="*" />
                        <RowDefinition Name="rowWorkshop" Height="*" />
                        <RowDefinition Name="rowExplosivesRoom" Height="*" />
                        <RowDefinition Name="rowSplinterResearchLab" Height="*" />
                        <RowDefinition Name="rowVault" Height="*" />
                        <RowDefinition Name="rowSparringRoom" Height="*" />
                        <RowDefinition Name="rowArmourersWorkshop" Height="*" />
                        <RowDefinition Name="rowJewellersWorkshop" Height="*" />
                        <RowDefinition Name="rowSurveyorsStudy" Height="*" />
                        <RowDefinition Name="rowGemcuttersWorkshop" Height="*" />
                        <RowDefinition Name="rowTormentCells" Height="*" />
                        <RowDefinition Name="rowStrongboxChamber" Height="*" />
                        <RowDefinition Name="rowHallOfMettle" Height="*" />
                        <RowDefinition Name="rowSacrificalChamber" Height="*" />
                        <RowDefinition Name="rowStorageRoom" Height="*" />
                        <RowDefinition Name="rowCorruptionChamber" Height="*" />
                        <RowDefinition Name="rowShrineOfEmpowerment" Height="*" />
                        <RowDefinition Name="rowTempestGenerator" Height="*" />
                        <RowDefinition Name="rowPoisionGarden" Height="*" />
                        <RowDefinition Name="rowTrapWorkshop" Height="*" />
                        <RowDefinition Name="rowPoolsOfRestoration" Height="*" />
                        <RowDefinition Name="rowFlameWorkshop" Height="*" />
                        <RowDefinition Name="rowLightningWorkshop" Height="*" />
                        <RowDefinition Name="rowHatchery" Height="*" />
                        <RowDefinition Name="rowRoyalMeetingRom" Height="*" />
                        <RowDefinition Name="rowIncursionLegend" Height="*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition x:Name="columnLevel1" Width="*" />
                        <ColumnDefinition x:Name="columnLevel2" Width="*" />
                        <ColumnDefinition x:Name="columnLevel3" Width="*" />
                        <ColumnDefinition x:Name="columnContains" Width="*" />
                        <ColumnDefinition x:Name="columnModifiers" Width="*" />
                    </Grid.ColumnDefinitions>
                    <!--  Header  -->
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="0"
                        Stroke="Black" />
                    <Label
                        Grid.Row="0"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Level 1"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="1"
                        Stroke="Black" />
                    <Label
                        Grid.Row="0"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Level 2"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="2"
                        Stroke="Black" />
                    <Label
                        Grid.Row="0"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Level 3"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="3"
                        Stroke="Black" />
                    <Label
                        Name="labelIncursionHeaderContains"
                        Grid.Row="0"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static incursion:IncursionResources.HeaderContains}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="4"
                        Stroke="Black" />
                    <Label
                        Name="labelIncursionHeaderModifiers"
                        Grid.Row="0"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static incursion:IncursionResources.HeaderModifiers}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <!--  Legend  -->
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="0"
                        Stroke="Black" />
                    <Label
                        x:Name="labelIncrusionLengend"
                        Grid.Row="26"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static incursion:IncursionResources.Legend}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="1"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static r:LeagueResources.LegendMoreValuable}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource HighValueColor}" />
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="2"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static r:LeagueResources.LegendValuable}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource MediumValueColor}" />
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="3"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static r:LeagueResources.LegendLessValuable}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource LowValueColor}" />
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="4"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static r:LeagueResources.LegendNotValuable}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource VeryLowValueColor}" />
                    <!--  Guardhouse  -->
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="0"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockGuardhouse"
                        Grid.Row="1"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.Guardhouse}" />
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="1"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockBarracks"
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.Barracks}"/>
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="2"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockHallOfWar"
                        Grid.Row="1"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
