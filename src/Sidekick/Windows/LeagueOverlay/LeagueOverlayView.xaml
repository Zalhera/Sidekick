                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockGuardhouseModifiers"
                        Grid.Row="1"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.GuardhouseModifiers}"/>
                    <!--  Workshop  -->
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="0"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockWorkshop"
                        Grid.Row="2"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.Workshop}"/>
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="1"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockEngineeringDepartment"
                        Grid.Row="2"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.EngineeringDepartment}"/>
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="2"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockFactory"
                        Grid.Row="2"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.Factory}"/>
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockWorkshopContains"
                        Grid.Row="2"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.WorkshopContains}"/>
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockWorkshopModifiers"
                        Grid.Row="2"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.WorkshopModifiers}"/>
                    <!--  Explosives Room  -->
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="0"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockExplosivesRoom"
                        Grid.Row="3"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.ExplosivesRoom}"/>
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="1"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockDemolitionLab"
                        Grid.Row="3"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.DemolitionLab}"/>
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="2"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockShrineOfUnmaking"
                        Grid.Row="3"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.ShrineOfUnmaking}"/>
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockExplosivesRoomContains"
                        Grid.Row="3"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.ExplosivesRoomContains}"/>
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Grid.Row="3"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <!--  Splinter Research Lab  -->
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="0"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSplinterReasearchLab"
                        Grid.Row="4"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.SplinterResearchLab}"/>
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="1"
                        Fill="{DynamicResource MediumValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockBreachContainmentChamber"
                        Grid.Row="4"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.BreachContainmentChamber}"/>
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="2"
                        Fill="{DynamicResource HighValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockHouseOfTheOthers"
                        Grid.Row="4"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.HouseOfOthers}"/>
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSplinterResearchLabContains"
                        Grid.Row="4"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.SplinterResearchLabContains}"/>
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <!--  Vault  -->
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="0"
                        Fill="{DynamicResource MediumValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockVault"
                        Grid.Row="5"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.Vault}"/>
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="1"
                        Fill="{DynamicResource MediumValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockTreasury"
                        Grid.Row="5"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.Treasury}"/>
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="2"
                        Fill="{DynamicResource HighValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockWealthOfTheVaal"
                        Grid.Row="5"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.WealthOfTheVaal}"/>
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockVaultContains"
                        Grid.Row="5"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.VaultContains}"/>
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Grid.Row="5"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
