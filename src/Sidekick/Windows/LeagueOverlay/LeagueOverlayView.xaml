                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSparringRoomModifiers"
                        Grid.Row="6"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}"
                        Text="{x:Static incursion:IncursionResources.SparringRoomModifiers}"/>
                    <!--  Armourer's Workshop  -->
